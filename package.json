{
    "name": "async-kv",
    "version": "1.1.2",
    "description": "Resolves promises in key-value pairs maintaining type information",
    "main": "build/main/index.js",
    "module": "build/module/index.js",
    "typings": "build/main/index.d.ts",
    "scripts": {
        "prebuild": "rm -rf build/*",
        "build": "run-p build:*",
        "build:main": "tsc -p tsconfig.json",
        "build:module": "tsc -p tsconfig.module.json",
        "fix": "run-s fix:*",
        "fix:prettier": "prettier \"src/**/*.ts\" --write",
        "fix:lint": "eslint src --ext .ts --fix",
        "test": "run-s build test:*",
        "test:jest": "jest --no-cache --runInBand",
        "test:lint": "eslint src --ext .ts",
        "test:prettier": "prettier \"src/**/*.ts\" --list-different",
        "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
        "watch:build": "tsc -p tsconfig.json -w",
        "watch:test": "jest --no-cache --runInBand --watch",
        "cov": "run-s build cov:*",
        "cov:test": "jest --coverage --no-cache --runInBand",
        "version": "standard-version",
        "prepare-release": "npm i && run-s test cov version"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "build/main",
        "build/module",
        "!**/*.spec.*",
        "!**/*.json",
        "CHANGELOG.md",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "promise",
        "typescript",
        "npm",
        "package",
        "key-value",
        "kv",
        "key",
        "value",
        "resolve",
        "object",
        "map",
        "set"
    ],
    "author": "Tony Tamplin <github@tony.dev>",
    "license": "MIT",
    "homepage": "https://github.com/tonytamps/async-kv",
    "repository": {
        "type": "git",
        "url": "git@github.com:tonytamps/async-kv.git"
    },
    "bugs": {
        "url": "https://github.com/tonytamps/async-kv/issues"
    },
    "engines": {
        "node": ">=14"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^4.0.1",
        "cspell": "^4.1.0",
        "eslint": "^7.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.22.0",
        "jest": "^27.2.0",
        "npm-run-all": "^4.1.5",
        "open-cli": "^6.0.1",
        "prettier": "^2.1.1",
        "standard-version": "^9.0.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    }
}
